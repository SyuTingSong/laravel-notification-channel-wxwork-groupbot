{
    "name": "laravel-notification-channels/wxwork-group-bot",
    "description": "Notification channel for WxWork GroupBot",
    "homepage": "https://github.com/laravel-notification-channels/wxwork-group-bot",
    "license": "MIT",
    "authors": [
        {
            "name": "TingSong-Syu",
            "email": "rek@rek.me",
            "homepage": "https://xts.so",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "guzzlehttp/guzzle": "^7.2",
        "illuminate/notifications": "~6.0 || ~7.0 || ~8.0",
        "illuminate/support": "~6.0 || ~7.0 || ~8.0",
        "ext-gd": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\WxWorkGroupBot\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\WxWorkGroupBot\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-text --coverage-clover=coverage.clover"
    },
    "config": {
        "sort-packages": true
    }
}
